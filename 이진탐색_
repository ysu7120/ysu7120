].sort()\n","    reverse_array[i].sort()\n","\n","  for q in queries:\n","    if q[0] != '?':\n","      res = count_by_range(array[len(q)],q.replace('?','a'),q.replace('?','z'))\n","    else:\n","      res = count_by_range(reverse_array[len(q)],q[::-1].replace('?','a'),q[::-1].replace('?','z'))\n","    answer.append(res)\n","  return answer"],"metadata":{"id":"z9-INY68pWfe"},"execution_count":null,"outputs":[]},{"cell_type":"markdown","source":["2회독"],"metadata":{"id":"tLRptz_DMBt6"}},{"cell_type":"code","source":["n=int(input())\n","array_n=list(map(int,input().split()))\n","m=int(input())\n","array_m=list(map(int,input().split()))\n","\n","for i in range(m):\n","  if array_m[i] in array_n:\n","    print(\"yes\")\n","  else:\n","    print(\"No\") "],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"RY9WSTalA1A0","executionInfo":{"status":"ok","timestamp":1658931665381,"user_tz":-540,"elapsed":15400,"user":{"displayName":"유성운","userId":"04010736311021015655"}},"outputId":"a2a38281-5c47-45ed-9a1f-efe274960e79"},"execution_count":3,"outputs":[{"output_type":"stream","name":"stdout","text":["5\n","8 3 7 9 2\n","3\n","5 7 9\n","No\n","yes\n","yes\n"]}]},{"cell_type":"code","source":["#순차탐색버전 떡자르기\n","n,m=map(int,input().split())\n","array=list(map(int,input().split()))\n","g=[]\n","result=0\n","k=max(array)\n","while True:\n","  for i in range(n):\n","    j=array[i]-k\n","    if j<0:\n","      j=0\n","    g.append(j)\n","  result+=sum(g)\n","  if m<=result:\n","    break\n","  else:\n","    result=0\n","    g=[]\n","    k-=1\n","\n","print(k)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"ovnjF8v1C-5f","executionInfo":{"status":"ok","timestamp":1658933380727,"user_tz":-540,"elapsed":7102,"user":{"displayName":"유성운","userId":"04010736311021015655"}},"outputId":"ff1271ff-58f0-4c11-bf1a-6a84717bd7f9"},"execution_count":15,"outputs":[{"output_type":"stream","name":"stdout","text":["4 6\n","19 15 10 17\n","15\n"]}]},{"cell_type":"code","sou